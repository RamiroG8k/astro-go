	},
	"dependencies": {
		"astro": "^5.16.15"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.6",
		"@biomejs/biome": "2.3.13",
		"husky": "^9.1.7",
		"typescript": "^5.9.3"
	}
}
